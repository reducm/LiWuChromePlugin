{
  "name":"__MSG_extension_name__",
  "version":"4.0",
  "manifest_version": 2,
  "default_locale": "en",
  "icons":{
    "16":"images/icon16.png",
    "19":"images/icon19.png",
    "48":"images/icon48.png",		
    "128":"images/icon128.png"		
  },
  "background": {
    "scripts":["js/background.js"]
  },
  "description":"__MSG_extension_description__",
  "permissions": [ 
    "https://www.253874.com/*",
    "http://sunyanzi.ruanmeizi.com/*",		
    "tabs",		
    "unlimitedStorage" 
  ],
  "web_accessible_resources":[
	"images/animated-overlay.gif",
	"images/ui-bg_flat_0_aaaaaa_40x100.png",
	"images/ui-bg_flat_75_ffffff_40x100.png",
	"images/ui-bg_glass_55_fbf9ee_1x400.png",
	"images/ui-bg_glass_65_ffffff_1x400.png",
	"images/ui-bg_glass_75_dadada_1x400.png",
	"images/ui-bg_glass_75_e6e6e6_1x400.png",
	"images/ui-bg_glass_95_fef1ec_1x400.png",
	"images/ui-bg_highlight-soft_75_cccccc_1x100.png",
	"images/ui-icons_2e83ff_256x240.png",
	"images/ui-icons_222222_256x240.png",
	"images/ui-icons_454545_256x240.png",
	"images/ui-icons_888888_256x240.png",
	"images/ui-icons_cd0a0a_256x240.png",
	"images/leftbg.jpg",
	"images/ajax-loader.gif",
	"images/jquery.wysiwyg.bg.png",
	"images/jquery.wysiwyg.gif"
  ],
  "page_action":{
    "default_icon": "images/icon19.png",
    "default_title": "__MSG_extension_name__",
    "default_popup": "html/popup.html"
  },
  "homepage_url":"https://github.com/reducm/LiWuChromePlugin",
  "content_scripts":[
    {
    "matches" : [        
      "https://www.253874.com/*"
    ],
    "js" : ["js/libs/jquery.min.js","js/libs/jquery-ui.min.js","js/liwu.js","js/libs/jquery.scrollTo.min.js","js/libs/jquery.wysiwyg.js"],
	"css": ["css/jquery-ui.min.css","css/style.css", "css/jquery.wysiwyg.css"],
    "run_at" : "document_idle",
    "all_frames" : true
    }
  ]
}
